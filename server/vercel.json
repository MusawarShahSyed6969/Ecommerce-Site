{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ],
  "includeFiles": [
    "server/middleware/**",
    "server/controllers/**",
    "server/models/**"
  ]
}
